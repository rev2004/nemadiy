<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="nema-temp-repositoryPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>org.imirsel.nema.repository.File</class>
    <class>org.imirsel.nema.repository.Dataset</class>
    <class>org.imirsel.nema.repository.Track</class>
    <class>org.imirsel.nema.repository.Set1</class>
    <class>org.imirsel.nema.repository.Collection</class>
    <class>org.imirsel.nema.repository.NEMAFile</class>
    <class>org.imirsel.nema.repository.NEMADataset</class>
    <class>org.imirsel.nema.repository.NEMATrack</class>
    <class>org.imirsel.nema.repository.NEMACollection</class>
    <class>org.imirsel.nema.repository.NEMASet</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:mysql://nema.lis.uiuc.edu:3306/nemadatarepository"/>
      <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
      <property name="hibernate.connection.password" value=""/>
      <property name="hibernate.connection.username" value=""/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
</persistence>
